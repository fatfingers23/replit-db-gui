{
  "root": true,
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": {
      "ts": "@typescript-eslint/parser",
      "<template>": "espree"
    }
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:vue/base",
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:vue/vue3-essential",
    "@vue/eslint-config-typescript"
  ],
  "rules": {
    "quotes": ["error","single"],
    "semi": 2
  }
}